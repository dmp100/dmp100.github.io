{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Layout",
          "name": "layout",
          "type": "string",
          "default": "post"
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Project",
          "name": "project",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.preview.pathName": "/:categories/:year/:month/:day/:title:output_ext",
  "frontMatter.content.pageFolders": [
    {
      "title": "drafts",
      "path": "[[workspace]]/_drafts"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/_posts"
    },
    {
      "path": "[[workspace]]/pages"
    },
    {
      "path": "[[workspace]]/_posts/ideas"
    },
    {
      "path": "[[workspace]]/_posts/drawing1"
    },
    {
      "path": "[[workspace]]/_posts/diary"
    },
    {
      "path": "[[workspace]]/projects"
    }
  ],
  "frontMatter.git.enabled": true
}